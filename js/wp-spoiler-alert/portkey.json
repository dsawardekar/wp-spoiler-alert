{
  "app/ext/*.js": {
    "type": "external",
    "test": "test/ext/%sTest.js"
  },
  "test/ext/*Test.js": {
    "type": "test",
    "alternate": "app/ext/*.js"
  },

  "app/components/*.js": {
    "type": "component",
    "test": "test/components/%sTest.js"
  },
  "test/components/*Test.js": {
    "type": "test",
    "alternate": "app/components/%s.js"
  }
}

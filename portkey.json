{
  "lib/WpSpoilerAlert/*.php": {
    "type": "library",
    "test": "test/WpSpoilerAlert/%sTest.php"
  },
  "test/WpSpoilerAlert/*Test.php": {
    "type": "test",
    "alternate": "lib/WpSpoilerAlert/%s.php"
  },

  "lib/WordPress/*.php": {
    "type": "wordpress",
    "mapping": "s",
    "test": "test/WordPress/%sTest.php"
  },
  "test/WordPress/*Test.php": {
    "type": "test",
    "alternate": "lib/WordPress/%s.php"
  }
}
